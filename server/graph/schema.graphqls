# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  firstName: String!
  lastName: String!
  age: Int!
  createdAt: String!
  updatedAt: String!
  deletedAt: String!
}

type Query {
  users: [User!]!
}

input UserInput {
  firstName: String!
  lastName: String!
  age: String!
}

type Mutation {
  createUser(input: UserInput!): User!
}
