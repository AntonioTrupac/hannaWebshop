# GraphQL schema example
#
# https://gqlgen.com/getting-started/

# in the schema we will be doing Query and Mutations
schema {
  query: Query
  mutation: Mutation
}

type User {
  id: ID!
  firstName: String!
  lastName: String!
  password: String!
  age: Int!
}

type Address {
  id: ID!
  city: String!
  zipCode: Int!
}

type Query {
  users: [User!]!
}

input UserInput {
  firstName: String!
  lastName: String!
  password: String!
  age: Int!
}

type Mutation {
  createUser(input: UserInput!): User!
}
